package day;


import java.util.Scanner;

public class Main {    
	public  static void main(String[]arg) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int k=0;
		int a[][]=new int[n][n];
		for(int i=0;i<n;i++) {
			for (int j=0;j<n;j++) {
				a[i][j]=sc.nextInt();
				if(a[i][j]<10) {
				k+=a[i][j];	
					
				}
			}
			
		}
		System.out.println(k);
	}
	}
